<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bobo.normalman.bobobubble">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:name="com.bobo.normalman.bobobubble.BOBOBubbleApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.bobo.normalman.bobobubble.MainActivity"></activity>
        <activity android:name="com.bobo.normalman.bobobubble.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bobo.normalman.bobobubble.AuthActivity"></activity>
        <activity android:name="com.bobo.normalman.bobobubble.view.shot_detail.ShotDetailActivity"></activity>
        <activity android:name="com.bobo.normalman.bobobubble.view.user_detail.UserActivity"></activity>
        <activity android:name="com.bobo.normalman.bobobubble.view.shot_list.ShotListActivity"></activity>
        <activity android:name="com.bobo.normalman.bobobubble.view.bucket.BucketListActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>